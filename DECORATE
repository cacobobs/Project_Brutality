ACTOR NewClip : Ammo //Replaces Clip
{
	Inventory.PickUpSound "misc/clip_PickUp"
    Game Doom
	Inventory.PickUpMessage "Picked up a High Caliber Magazine"
	Inventory.Amount 15
    Inventory.MaxAmount 300
    Ammo.BackpackAmount 15
    Ammo.BackpackMaxAmount 600
    Inventory.Icon "CLIPA0"
    States
    {
    Spawn:
      CLIP A 2
      Loop
	}
}

ACTOR NewClipBox : NewClip //Replaces ClipBox
{
	Inventory.PickUpSound "cboxpkup"
    Game Doom
	Inventory.Amount 50
    Inventory.MaxAmount 300
    Ammo.BackpackAmount 50
    Ammo.BackpackMaxAmount 600
    Inventory.Icon "CLIPA0"
    States
    {
    Spawn:
      AMMO A 2
      Loop
	}
}

ACTOR NewClipBoxBig : NewClip //Replaces ClipBox
{
    Game Doom
	Inventory.Amount 80
    Inventory.MaxAmount 300
    Inventory.Icon "CLIPA0"
	Inventory.PickUpMessage "Picked up a large bullet case"
	Inventory.PickUpSound "cboxpkup"
	Ammo.BackpackAmount 80
    Ammo.BackpackMaxAmount 600
    States
    {
    Spawn:
	TNT1 A 0 ACS_ExecuteAlways(796, 0, 0, 0, 0)//Check if Vanilla Mode is activated
      8CLP A 2
      Loop
	  
	  ReplaceVanilla:
	TNT1 A 0 A_SpawnItemEx("NewClipBox",0,0,0,0,0,0,0,288)
	TNT1 A 1
	Stop
	}
}



