ACTOR LiquidAcidTrail: Brutal_FlyingBlood 
{
 scale 1.5
 gravity 0.3
 -THRUACTORS
 -NOCLIP
  -NOBLOCKMAP
  translation "168:191=112:127","16:47=120:127"
    States
    {
     Spawn:
	 TNT1 A 0
     BLOD B 15 A_SpawnItemEx("GreenTrailSparks", random(5,-5), random(5,-5), random(5,-5), 0, 0, 0, 0, 128, 0)
	 Stop
	 Death:
	   TNT1 A 0
       Stop
    }
}

