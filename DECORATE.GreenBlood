ACTOR CeilGreenXDeath1: Brutal_BloodSpot
{
  +SPAWNCEILING
  +MOVEWITHSECTOR
  -DOOMBOUNCE
  +NOGRAVITY
  +DontSplash
  +MISSILE
  Scale 0.7
 Renderstyle Normal 
 translation "168:191=112:127","16:47=120:127"
States
{
Spawn:
Goto Crash
Death:
Crash:
TNT1 A 0
TNT1 A 0 A_JUmp (255, "Live1", "Live2", "Live3")
Goto Live1

  Live1:
  XME2 FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF 3 ThrustThingZ(0,35,0,1)
  Goto Fall

  Live2:
  XME2 FFFFFFFFFFFFFFFFFFFFFFFFFFFF 3 ThrustThingZ(0,35,0,1)
  Goto Fall

  Live3:
  XME2 FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF 3 ThrustThingZ(0,35,0,1)
  Goto Fall

 Fall:
  TNT1 A 0 A_SpawnItem("GreenXDeath1NoStick")
  Stop
	Splash:
        BLOD A 0
        Stop
    }
}

ACTOR SmearingGreenXDeath1
{
    Radius 2
    Height 2
	Speed 10
	Mass 1
	Scale 0.7
    +NOBLOCKMAP
    +NOTELEPORT
	+THRUGHOST
	+CLIENTSIDEONLY
    +DONTSPLASH
    +MOVEWITHSECTOR
	-DONTSPLASH
	+FORCEXYBILLBOARD
	+QUARTERGRAVITY
	 translation "168:191=112:127","16:47=120:127"
    States
    {
	Spawn:
	TNT1 A 0
	TNT1 A 0 A_ChangeFlag("NOGRAVITY", 1)
	TNT1 A 0 A_Recoil(-25)
	TNT1 A 0 A_Jump(255, "Spawn1", "Spawn2", "Spawn3", "Spawn4")
	Goto Spawn1

	Spawn1:
	XME2 GGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGG 1 A_CheckFloor("Rest")
	Goto Death

	Spawn2:
	XME2 GGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGG 1 A_CheckFloor("Rest")
	Goto Death

	Spawn3:
	XME2 GGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGG 1 A_CheckFloor("Rest")
	Goto Death

	Spawn4:
	XME2 GGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGgg 1 A_CheckFloor("Rest")
	Goto Death

	Death:
	    TNT1 A 0
	    TNT1 A 0 A_SpawnItem("GreenXDeath1NoStick")
        Stop
	Rest:
	TNT1 A 0
	Stop
    }
}

ACTOR GreenXDeath1NoStick: XDeath1
{
    +CLIENTSIDEONLY
	+DONTSPLASH
    Speed 0
	Gravity 0.4
    DeathSound "misc/xdeath2"
	Decal BrutalBloodSplat
	Radius 1
	Height 0
	Scale 0.7
	translation "168:191=112:127","16:47=120:127"
