{
   DecayStart 3
   DecayTime 4
}

slider Drops
{
   DistY -320
   SlideStart 0.01
   SlideTime 15
}

Combiner WaterDropEffect
{
   LiquidFaderFast
   Drops
   BrutalBloodSmearer
}

