ACTOR Stimpack2 : Health
{
	Game Doom
	SpawnID 7050
	+FLOORCLIP
	+DONTGIB
	Inventory.Amount 10
	Inventory.Pickupsound "misc/N_HP_pickup"
	Inventory.PickupMessage "$GOTSTIM" // "Picked up a stimpack."
	States
	{
	Spawn:
		ST1M A -1
		Stop
	}
}

